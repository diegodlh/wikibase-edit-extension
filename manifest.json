{
  "description": "wikibase-edit sample extension",
  "manifest_version": 2,
  "name": "wikibase-edit-extension",
  "version": "1.0",
  "permissions": [
    "*://www.wikidata.org/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "wikibase-edit"
  }
}